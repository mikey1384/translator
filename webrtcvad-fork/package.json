{
  "name": "@stage5/webrtcvad",
  "author": "Serenade Labs, Inc. <contact@serenade.ai>",
  "version": "1.0.1-stage5.1",
  "description": "Bindings for the WebRTC VAD with prebuilt binaries for Electron.",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "keywords": ["webrtc", "vad", "audio", "pcm", "electron", "prebuilt"],
  "bugs": {
    "url": "https://github.com/mikey1384/translator/issues"
  },
  "repository": "git+https://github.com/mikey1384/translator.git",
  "dependencies": {
    "bindings": "^1.3.0",
    "node-addon-api": "^1.7.1",
    "prebuild-install": "^7.1.3"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepare": "tsc",
    "install": "prebuild-install || node-gyp rebuild",
    "prebuild": "prebuildify --napi --strip --targets @electron/35.0.0,@electron/35.5.1"
  },
  "devDependencies": {
    "@types/bindings": "^1.3.0",
    "@types/node": "^12.11.5",
    "typescript": "^3.6.4",
    "prebuildify": "^6.0.0"
  },
  "overrides": {
    "tar-fs": "^3.0.9",
    "prebuild-install": {
      "tar-fs": "^3.0.9"
    }
  },
  "binary": {
    "module_name": "webrtcvad",
    "napi_versions": [8, 9, 10],
    "package_name": "{module_name}-v{version}-{node_abi}-{platform}-{arch}.tar.gz"
  },
  "gypfile": true
}
