{
  "name": "@app/main",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "boot.mjs",
  "scripts": {
    "build": "npx tsx build.ts",
    "build:dev": "npm run build",
    "build:prod": "npm run build",
    "build:watch": "npx chokidar \"index.ts\" \"**/*.ts\" -c \"npm run build\" --initial",
    "dev": "npm run build:dev && (npm run build:watch & npx electron .)",
    "start": "npx electron ."
  },
  "dependencies": {
    "electron": "35.0.0"
  },
  "devDependencies": {}
}
