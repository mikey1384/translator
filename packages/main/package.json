{
  "name": "@app/main",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "boot.mjs",
  "scripts": {
    "build": "bunx tsx build.ts",
    "build:dev": "bun run build",
    "build:prod": "bun run build",
    "build:watch": "bunx chokidar \"index.ts\" \"**/*.ts\" -c \"bun run build\" --initial",
    "dev": "bun run build:dev && (bun run build:watch & bunx electron .)",
    "start": "bunx electron ."
  },
  "dependencies": {
    "electron": "35.0.0"
  },
  "devDependencies": {}
}
